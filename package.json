{
  "name": "express-graphql-server",
  "version": "1.0.0",
  "description": "A GraphQL server based on Apollo Server and Express.js",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha __tests__/server.spec.js",
    "lint": "./node_modules/.bin/eslint src __tests__",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simplymichael/express-graphql-server.git"
  },
  "keywords": [
    "apollo",
    "express",
    "graphql",
    "server",
    "apollo-server",
    "express-server",
    "graphql-server"
  ],
  "author": "simplymichael <simplymichaelorji@gmail.com> (https://github.com/simplymichael)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simplymichael/express-graphql-server/issues"
  },
  "homepage": "https://github.com/simplymichael/express-graphql-server#readme", 
  "dependencies": {
    "@graphql-tools/schema": "9.0.13",
    "apollo-server": "3.11.1",
    "apollo-server-express": "3.11.1",
    "connect-redis": "6.1.3",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "graphql": "16.6.0",
    "redis": "4.5.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "chai": "4.3.7",
    "commitizen": "4.2.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.31.0",
    "ghooks": "2.0.4",
    "mocha": "10.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  }
}
